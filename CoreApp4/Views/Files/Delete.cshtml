@model YllariFM.Models.DB.File

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <h2>Borrar File</h2>
        <div class="box box-danger">
            <div class="box-body">
                <h3>¿Estas seguro que deseas eliminar el file @Model.Codigo?</h3>
                <div>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Codigo)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Codigo)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Descripcion)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Descripcion)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.IdAgenciaNavigation)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.IdAgenciaNavigation.CorreoContacto)
                        </dd>
                        @*
                            <dt>
                                @Html.DisplayNameFor(model => model.IdBibliaNavigation)
                            </dt>
                            <dd>
                                @Html.DisplayFor(model => model.IdBibliaNavigation.IdBiblia)
                            </dd>
                        *@
                    </dl>
                </div>
                <div class="box-footer">
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="IdFile" />
                        <button type="button" class="btn btn-default" onclick="window.location.href='/Files'">Cancelar</button>
                        <input type="submit" value="Eliminar" class="btn btn-danger pull-right" />
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>